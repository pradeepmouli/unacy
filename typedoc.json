{
  "entryPoints": ["packages/*/src/index.ts"],
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "includeVersion": true,
  "readme": "none",
  "githubPages": false,
  "globalsOrder": "alphabetical",
  "membersOrder": "alphabetical",
  "parametersOrder": "alphabetical",
  "enumMembersOrder": "alphabetical",
  "treatWarningsAsErrors": false,
  "disableSources": false,
  "excludePrivate": true,
  "excludeProtected": false,
  "hideGenerator": true,
  "maxTypeConversionDepth": 4,
  "stripInternal": true,
  "navigation": {
    "includeCategories": true
  }
}
